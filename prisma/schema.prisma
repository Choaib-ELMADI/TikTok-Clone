generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Video {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  description String
  likes       Int
  owner       String
  saves       Int
  tags        String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@map("videos")
}
